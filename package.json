{
  "name": "glenndehaan.com",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production --config ./_scripts/webpack.config.js --optimize-minimize",
    "start": "webpack --watch --mode development --config ./_scripts/webpack.config.js"
  },
  "engines": {
    "node": "^8.0.0"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ],
      "react/prop-types": 0
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "pragma": "h"
      }
    },
    "globals": {
      "System": true
    }
  },
  "eslintIgnore": [
    "src/dist"
  ],
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "ejs": "^2.6.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-react": "^7.10.0",
    "express": "^4.16.3",
    "express-browsersupport": "^1.3.4",
    "gsap": "^2.0.1",
    "highlight.js": "^9.12.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mitt": "^1.1.2",
    "node-sass": "^4.9.1",
    "preact": "^8.2.9",
    "preact-router": "^2.6.1",
    "preact-transition-group": "^1.1.1",
    "sass-loader": "^7.0.3",
    "webpack": "^4.15.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.0.8"
  }
}
