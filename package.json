{
  "name": "glenndehaan.com",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production --config ./_scripts/webpack.config.js --optimize-minimize",
    "start": "webpack --watch --mode development --config ./_scripts/webpack.config.js",
    "lint": "eslint -c ./package.json ./"
  },
  "engines": {
    "node": "^8.0.0"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-console": 0,
      "react/prop-types": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "pragma": "h",
        "version": "16.0"
      }
    },
    "globals": {
      "System": true
    }
  },
  "eslintIgnore": [
    "public/dist"
  ],
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-browsersupport": "^1.3.5",
    "gsap": "^2.0.2",
    "highlight.js": "^9.13.1",
    "mini-css-extract-plugin": "^0.4.4",
    "mitt": "^1.1.2",
    "node-fetch": "^2.2.1",
    "node-sass": "^4.10.0",
    "preact": "^8.3.1",
    "sass-loader": "^7.1.0",
    "webpack": "^4.25.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.2"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.11.1"
  }
}
